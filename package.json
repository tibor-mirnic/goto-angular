{
  "name": "goto-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-s build:scripts serve:proxy-config",
    "serve:proxy-config": "ng serve --proxy-config scripts/proxy/config.js",
    "build": "run-s build:angular build:scripts apply:environment",
    "build:angular": "ng build --prod",
    "build:scripts": "tsc -p scripts",
    "apply:environment": "node scripts/environment/apply-environment.js",
    "apply:version": "node scripts/versioning/apply-version.js",
    "test": "ng test",
    "lint": "ng lint goto-angular",
    "e2e": "ng e2e"
  },
  "private": true,
